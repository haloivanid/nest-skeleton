{
  "name": "@mevaspace/nest-skeleton",
  "version": "0.0.0-dev",
  "description": "nothing",
  "author": "ivan azis <ivanazis.id@gmail.com>",
  "license": "MIT",
  "packageManager": "pnpm@10.26.1",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "commit": "pnpm prepare && pnpm cz",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "format": "prettier --write .",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "prepare": "husky",
    "typeorm": "node -r ts-node/register -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "db:migration:generate": "wrap () { pnpm typeorm -d src/library/utils/typeorm/typeorm-migrations.datasource.ts migration:generate src/databases/migrations/\"$*\" | cat; }; wrap",
    "db:migrate": "pnpm typeorm -d src/library/utils/typeorm/typeorm-migrations.datasource.ts migration:run",
    "db:migrate:revert": "pnpm typeorm -d src/library/utils/typeorm/typeorm-migrations.datasource.ts migration:revert",
    "db:seed:create": "wrap () { pnpm typeorm migration:create src/databases/migrations/\"seed-$*\" | cat; }; wrap"
  },
  "dependencies": {
    "@nestjs/cache-manager": "^3.1.0",
    "@nestjs/common": "^11.1.9",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.9",
    "@nestjs/cqrs": "^11.0.3",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-fastify": "^11.1.11",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.9",
    "@nestjs/typeorm": "^11.0.0",
    "@react-email/components": "^1.0.3",
    "@react-email/render": "^2.0.1",
    "@swc/cli": "^0.7.9",
    "@swc/core": "^1.15.7",
    "@types/passport-jwt": "^4.0.1",
    "bcryptjs": "^3.0.3",
    "cache-manager": "^7.2.7",
    "dayjs": "^1.11.19",
    "dotenv": "^17.2.3",
    "fastify": "5.6.2",
    "fastify-raw-body": "^5.0.0",
    "nanoid": "^5.1.6",
    "nodemailer": "^7.0.12",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "react": "19.2.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.5.4",
    "typeorm": "0.3.26",
    "uuid": "^13.0.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@eslint/js": "^9.39.2",
    "@nestjs/cli": "^11.0.14",
    "@types/node": "^25.0.3",
    "@types/nodemailer": "^7.0.4",
    "@types/react": "^19.2.7",
    "commitizen": "^4.3.1",
    "conventional-changelog-cli": "^5.0.0",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
